    <PROPERTY NAME="wordBreakChars" VALUE="=-+*/, " />
  </PROPS>
  
  <RULES ESCAPE="\" IGNORE_CASE="FALSE" HIGHLIGHT_DIGITS="TRUE"
      DIGIT_RE="[0-9]+">

  <SEQ TYPE="OPERATOR">-</SEQ>
  <SEQ TYPE="OPERATOR">+</SEQ>
  <SEQ TYPE="OPERATOR">//</SEQ>
  <SEQ TYPE="OPERATOR">/</SEQ>
  <SEQ TYPE="OPERATOR">=</SEQ>
  <SEQ TYPE="OPERATOR">:</SEQ>

  <SEQ_REGEXP HASH_CHARS="*" AT_WORD_START="TRUE" TYPE="COMMENT2">^*.*</SEQ_REGEXP>
  <SEQ_REGEXP HASH_CHARS="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789" AT_WORD_START="TRUE" TYPE="KEYWORD4">[0-9][BHF]</SEQ_REGEXP>
